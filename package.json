{
  "name": "blog-turborepo",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "turbo run dev",
    "dev:front": "turbo run dev --filter=front",
    "dev:api": "turbo run dev --filter=api",
    "build": "turbo run build",
    "build:front": "turbo run build --filter=front",
    "build:api": "turbo run build --filter=api",
    "start": "turbo run start",
    "start:api": "cd apps/api && npm run start:prod",
    "start:front": "cd apps/front && npm start",
    "lint": "turbo run lint",
    "lint:front": "turbo run lint --filter=front",
    "lint:api": "turbo run lint --filter=api",
    "db:generate": "turbo run db:generate --filter=api",
    "db:seed": "cd apps/api && npm run db:seed",
    "db:migrate": "cd apps/api && npx prisma migrate dev",
    "db:studio": "cd apps/api && npx prisma studio",
    "clean": "turbo run clean",
    "clean:front": "turbo run clean --filter=front",
    "clean:api": "turbo run clean --filter=api",
    "test": "turbo run test",
    "test:front": "turbo run test --filter=front",
    "test:api": "turbo run test --filter=api"
  },
  "packageManager": "npm@10.8.1",
  "workspaces": [
    "apps/*"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "turbo": "^2.3.0"
  }
}