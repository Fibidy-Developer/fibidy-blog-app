{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd apps/front && npm run build",
  "devCommand": "cd apps/front && npm run dev",
  "installCommand": "npm install",
  "outputDirectory": "apps/front/.next",
  "framework": "nextjs",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./apps/front/",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://your-railway-api-url.railway.app/:path*"
    }
  ]
}